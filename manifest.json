{
  "manifest_version": 2,
  "name": "Darkmode Brightspace",
  "version": "1.0",

  "description": "Adds a dark mode theme to Brightspace",

  "content_scripts": [
    {
      "matches": ["*://purdue.brightspace.com/*"],
      "js": ["darkmode.js", "test.js"],
      "css": ["darkmode.css"]
    }
  ]
}
